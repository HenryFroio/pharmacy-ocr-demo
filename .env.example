# Firebase Configuration - DEMO
REACT_APP_FIREBASE_API_KEY=your-firebase-api-key-here
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project-demo.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-demo
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project-demo.firebasestorage.app
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=123456789012
REACT_APP_FIREBASE_APP_ID=1:123456789012:web:your-app-id-here
REACT_APP_FIREBASE_MEASUREMENT_ID=G-YOUR-MEASUREMENT-ID

# Azure Cognitive Services OCR - DEMO
REACT_APP_AZURE_OCR_ENDPOINT=https://your-region.cognitiveservices.azure.com/
REACT_APP_AZURE_OCR_KEY=your-azure-ocr-subscription-key-here

# Application Settings
REACT_APP_APP_NAME=Pharmacy OCR Demo
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=demo

# Optional: Backup OCR Service
REACT_APP_TESSERACT_WORKER_PATH=/path/to/tesseract/worker
REACT_APP_ENABLE_FALLBACK_OCR=true

# Development
GENERATE_SOURCEMAP=false
SKIP_PREFLIGHT_CHECK=true